# Discord Bot Configuration
# These environment variables will override values in config.yaml

# Required: Discord bot token
DISCORD_TOKEN=your_discord_bot_token_here

# Optional: API keys for dictionary services
DICTIONARY_API_KEY=your_dictionary_api_key_here
URBAN_API_KEY=your_urban_dictionary_api_key_here

# Optional: Bot configuration
COMMAND_PREFIX=!
DB_FILE=db